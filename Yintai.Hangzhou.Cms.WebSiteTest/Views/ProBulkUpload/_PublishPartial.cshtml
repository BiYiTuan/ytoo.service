@model IEnumerable<Yintai.Hangzhou.Cms.WebSiteTest.Util.ProductPublishResult>
@if( Model.Count() >0) {
    <tr>
        <th>
            商品编码
        </th>
        <th>验证状态</th>
        <th>验证结果</th>
    </tr>

    foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ItemCode)
        </td>
         <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
         <td>
            @Html.DisplayFor(modelItem => item.PublishMemo)
        </td>
       
    </tr>
    }
    <tr><td colspan="3">@Html.ActionLink("继续发布","Display",null,new{ @class="btn btnprimary"})</td></tr>
} else {
    <tr> <td>
        <h4>没有商品发布成功！</h4>
         </td>
    </tr>
}

