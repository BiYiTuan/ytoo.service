<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <section name="imageSetting" type="Yintai.Architecture.ImageTool.Configurations.ImageSettingConfig, Yintai.Architecture.ImageTool" />
  </configSections>
  <!--folder文件在服务器上目录-->
  <imageSetting folder="D:\Websites\File\" imageQuality="90" backupFolder="D:\Websites\File\Backup\">
    <!--key文件上传键名 suffix允许上传的文件类型 size允许上传的文件大小 folder文件上传的相对目录,目前只提供日期分级(年:{Year}月:{Month}日:{Day})-->
    <image key="promotion" suffix="jpg,jpeg,gif,bmp,png" size="2048" folder="pro/{Year}{Month}{Day}/" rootFolder="">
      <thumb key="1" mode="W" width="120" height="0" />
      <thumb key="2" mode="W" width="320" height="0" />
    </image>
    <image key="FeikeImage" suffix="jpg,jpeg,gif,bmp,png" size="2048" folder="Feike/{Year}{Month}{Day}/" rootFolder="">
      <thumb key="1" mode="NHW" width="120" height="120" />
      <thumb key="2" mode="NHW" width="600" height="600" />
      <thumb key="3" mode="NHW" width="800" height="600" />
      <thumb key="4" mode="NHW" width="176" height="220" />
      <thumb key="5" mode="NHW" width="240" height="320" />
    </image>
    <image key="UserPortrait" suffix="jpeg,jpg,gif,bmp,png" size="2560" folder="UserPortrait/{0}/{1}/">
      <thumb key="1" mode="Cut" width="96" height="96" />
      <thumb key="2" mode="Cut" width="48" height="48" />
      <thumb key="3" mode="Cut" width="180" height="180" />
      <thumb key="4" mode="Cut" width="120" height="120" />
    </image>
    <image key="OriginalPortrait" suffix="jpeg,jpg,gif,bmp,png" size="2560" folder="userportraitoriginal/{0}/{1}/">
      <thumb key="1" mode="Cut" width="305" height="305" />
    </image>
    <image key="CMSUploadImage" suffix="jpg,gif,bmp,png,swf" size="2048" folder="CMS/{Year}{Month}{Day}/"></image>
    <image key="FetionForum" suffix="jpg,gif,bmp,png" size="2048" folder="bbs/{Year}{Month}{Day}/">
      <thumb key="2" mode="NHW" width="700" height="700" />
    </image>
    <image key="FetionFile" size="2048" folder="file/{0}/" asNormalFile="true"></image>
    <image key="WebImage" suffix="jpeg,jpg,gif,bmp,png" size="2048" folder="web/{Year}{Month}{Day}/" asNormalFile="true"></image>
    <image key="FetionLivingPortrait" suffix="jpeg,jpg,gif,bmp,png" size="2048" folder="FetionLivingPortrait/{Year}{Month}{Day}/">
      <thumb key="1" mode="NHW" width="48" height="48" />
    </image>
    <image key="FetionLivingAdv" suffix="jpeg,jpg,gif,bmp,png" size="2048" folder="FetionLivingAdv/{Year}{Month}{Day}/">
      <thumb key="1" mode="NHW" width="142" height="92" />
    </image>
    <image key="FetionLiving" suffix="jpeg,jpg,gif,bmp,png" size="1024" folder="FetionLiving/{Year}{Month}{Day}/">
      <thumb key="1" mode="NHW" width="100" height="100" />
    </image>
    <image key="FetionLivingPropagandize" suffix="jpeg,jpg,gif,bmp,png" size="2048" folder="FetionLivingPropagandize/{Year}{Month}{Day}/">
      <thumb key="1" mode="NHW" width="500" height="300" />
    </image>
    <image key="GroupAlbumImage" suffix="jpeg,jpg,gif,bmp,png" size="2048" folder="GroupAlbumImage/{Year}{Month}{Day}/">
      <thumb key="1" mode="NHW" width="600" height="600" />
      <thumb key="2" mode="NHW" width="120" height="120" />
      <thumb key="3" mode="NHW" width="50" height="47" />
    </image>
    <image key="GroupTopicImage" suffix="jpeg,jpg,gif,bmp,png" size="300" folder="GroupTopicImage/{Year}{Month}{Day}/">
      <thumb key="1" mode="NHW" width="180" height="180" />
    </image>
    <image key="GroupBannerImage" suffix="jpeg,jpg,gif,bmp,png" size="300" folder="GroupBannerImage/{Year}{Month}{Day}/">
      <thumb key="1" mode="NHW" width="448" height="78" />
    </image>
    <image key="huodong" suffix="jpg,jpeg,gif,bmp,png" size="2048" folder="Huodong/{Year}{Month}{Day}/"></image>
    <image key="GroupPortrait" suffix="jpg" size="2048" folder="group2/head/" asNormalFile="true"></image>
  </imageSetting>
  <log4net configSource="Configurations\log4net.config" />
  <appSettings configSource="Configurations\appSettings.config" />
  <connectionStrings>
    <!--<add name="YintaiHangzhouContext" connectionString="Data Source=.;Initial Catalog=YintaiHangzhou;Persist Security Info=True;User ID=sa;Password=!QA2ws;MultipleActiveResultSets=True"
     providerName="System.Data.SqlClient" />-->
    <add name="YintaiHangzhouContext" connectionString="Data Source=BJDBS002\JHIDCDBS009;Initial Catalog=YintaiHzhou;Persist Security Info=True;User ID=APP_Hzhou;Password=!QA2ws;MultipleActiveResultSets=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.5">
      <assemblies>
        <add assembly="System.Web.Abstractions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Web.Helpers, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Web.Routing, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Web.Mvc, Version=3.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
        <add assembly="System.Web.WebPages, Version=1.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
      </assemblies>
    </compilation>
    <customErrors mode="Off" />
    <authentication mode="Forms" />
    <authorization>
      <allow users="*" />
    </authorization>
    <pages controlRenderingCompatibilityVersion="4.0">
      <namespaces>
        <add namespace="System.Web.Helpers" />
        <add namespace="System.Web.Mvc" />
        <add namespace="System.Web.Mvc.Ajax" />
        <add namespace="System.Web.Mvc.Html" />
        <add namespace="System.Web.Routing" />
        <add namespace="System.Web.WebPages" />
      </namespaces>
    </pages>
    <httpModules>
      <add name="GlobalErrorHandlerModule" type="Yintai.Architecture.Common.Web.HttpModules.GlobalErrorHandlerModule, Yintai.Architecture.Common" />
      <add name="RequstLogging" type="Yintai.Architecture.Common.Web.HttpModules.RequestLogginHttpModule, Yintai.Architecture.Common" />
    </httpModules>
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <modules runAllManagedModulesForAllRequests="true">
      <add name="RequstLogging" type="Yintai.Architecture.Common.Web.HttpModules.RequestLogginHttpModule,Yintai.Architecture.Common" />
      <add name="GlobalErrorHandlerModule" type="Yintai.Architecture.Common.Web.HttpModules.GlobalErrorHandlerModule,Yintai.Architecture.Common" />
    </modules>
    <handlers>
      <add name="MVC_Routing" path="*.*" verb="*" modules="IsapiModule" scriptProcessor="C:\Windows\Microsoft.NET\Framework\v4.0.30319\aspnet_isapi.dll" resourceType="Unspecified" preCondition="classicMode,runtimeVersionv4.0,bitness32" />
    </handlers>
  </system.webServer>
  <system.data>
    <DbProviderFactories>
      <add name="EF Tracing Data Provider" invariant="EFTracingProvider" description="Tracing Provider Wrapper" type="EFTracingProvider.EFTracingProviderFactory, EFTracingProvider, Version=1.0.0.0, Culture=neutral, PublicKeyToken=def642f226e0e59b" />
      <add name="EF Generic Provider Wrapper" invariant="EFProviderWrapper" description="Generic Provider Wrapper" type="EFProviderWrapperToolkit.EFProviderWrapperFactory, EFProviderWrapperToolkit, Version=1.0.0.0, Culture=neutral, PublicKeyToken=def642f226e0e59b" />
    </DbProviderFactories>
  </system.data>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="System.Web.Mvc" publicKeyToken="31bf3856ad364e35" />
        <bindingRedirect oldVersion="1.0.0.0-2.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="EntityFramework" publicKeyToken="b77a5c561934e089" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.4.0.0" newVersion="4.4.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="ImageServiceBinding" maxReceivedMessageSize="65535000"></binding>
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:9100/File/Upload/Image" binding="basicHttpBinding" bindingConfiguration="ImageServiceBinding" contract="Yintai.Architecture.ImageTool.Contract.IImageService" name="ImageServiceEndpoint" />
    </client>
  </system.serviceModel>
</configuration>